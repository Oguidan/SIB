#: kivy 2.1.0
#: import ScreenManager kivy.uix.screenmanager.ScreenManager
#: import Screen kivy.uix.screenmanager.ScreenManager
#: import NewGameScreen screen
#: import LoadGameScreen screen
#: import GererLesListes screen
#: import MDTopAppBar kivymd.uix.toolbar

ScreenManager:
    name: "manager"
    current:
    id: screen_manager
    #transition: FadeTransition()
    GererLesListes:
        id: Gerer_screen
        name: 'GererLesListes'
        manager: 'screen_manager'
    ImprimerDesFiches:
        id: imprimer_screen
        name: 'ImprimerDesFiches'
        manager: 'screen_manager'
<GererLesListes>:
    MDIconButton:
        name:"screen 1"
        text:"Gérer les listes"
        icon:"database"
        pos_hint: {'center_x': 0.2,'center_y': 0.1}
    MDIconButton:
        name:"screen 2"
        text:"Imprimer des fiches"
        icon:"printer-search"
        pos_hint: {'center_x': 0.8,'center_y': 0.1}
<ImprimerDesFiches>:
    MDIconButton:
        name:"screen 1"
        text:"Gérer les listes"
        icon:"database"
        pos_hint: {'center_x': 0.2,'center_y': 0.1}
        on_press: app.root.current = "GererLesListes"
    MDIconButton:
        name:"screen 2"
        text:"Imprimer des fiches"
        icon:"printer-search"
        pos_hint: {'center_x': 0.8,'center_y': 0.1}